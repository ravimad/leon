#!/bin/bash --posix
SCALACLASSPATH="/home/kandhada/leon/leon/src/main/resources:/home/kandhada/leon/leon/target/scala-2.10/classes:/home/kandhada/leon/leon/library/target/scala-2.10/classes:/home/kandhada/leon/leon/unmanaged/64/cafebabe_2.10-1.2.jar:/home/kandhada/leon/leon/unmanaged/64/scalaz3.jar:/home/kandhada/leon/leon/unmanaged/64/insynth_2.10-2.1.jar:/home/kandhada/leon/leon/unmanaged/64/vanuatoo_2.10-0.1.jar:/home/kandhada/.sbt/boot/scala-2.10.2/lib/scala-library.jar:/home/kandhada/.sbt/boot/scala-2.10.2/lib/scala-compiler.jar:/home/kandhada/.ivy2/cache/org.scala-lang/scala-reflect/jars/scala-reflect-2.10.2.jar:/home/kandhada/.ivy2/cache/com.typesafe.akka/akka-actor_2.10/jars/akka-actor_2.10-2.2.0.jar:/home/kandhada/.ivy2/cache/com.typesafe/config/bundles/config-1.0.2.jar"

source /home/kandhada/leon/leon/./setupenv
java -Xmx5G -Xss1G -classpath ${SCALACLASSPATH} -Dscala.home="$SCALA_HOME" -Dscala.usejavacp=true scala.tools.nsc.MainGenericRunner -classpath ${SCALACLASSPATH} leon.smtlib.SmtlibToLeon $@ 2>&1 | tee last.log
